{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "name": "Ticket",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "active": {
      "type": "boolean"
    },
    "client": {
      "via": "tickets",
      "model": "client"
    },
    "city": {
      "via": "tickets",
      "model": "city"
    },
    "tickettype": {
      "model": "tickettype"
    },
    "assiganted": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "tickets"
    },
    "details": {
      "type": "string"
    },
    "ticketdetails": {
      "collection": "ticketdetail",
      "via": "ticket"
    },
    "answered": {
      "type": "boolean",
      "default": false
    },
    "escalated": {
      "type": "boolean",
      "default": false
    },
    "escalatedoffice": {
      "type": "boolean",
      "default": false
    }
  }
}
